{
  "author": "Evan Tahler <evan@delicioushat.com>",
  "name": "@actionhero/websocket",
  "description": "The Actionhero Websocket Server Plugin",
  "version": "22.0.0",
  "homepage": "http://www.actionherojs.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/actionhero.git"
  },
  "keywords": [
    "websocket",
    "socket",
    "realtime"
  ],
  "dependencies": {
    "browser_fingerprint": "^2.0.1",
    "dot-prop": "^5.2.0",
    "etag": "^1.8.1",
    "formidable": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "optionalDependencies": {},
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "scripts": {
    "test": "jest",
    "prepare": "npm run build",
    "pretest": "npm run lint && npm run build",
    "dev": "ts-node-dev --transpile-only --no-deps ./src/server",
    "build": "rm -rf dist && ./node_modules/.bin/tsc --declaration",
    "lint": "prettier --check src/**/*.ts __test__/**/*.ts",
    "pretty": "prettier --write src/**/*.ts __test__/**/*.ts"
  }
